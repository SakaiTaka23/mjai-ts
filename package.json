{
  "name": "mjai-ts",
  "version": "0.1.4",
  "description": "TypeScript library for parsing and managing mjai format mahjong game logs with core game state management",
  "license": "MIT",
  "keywords": [
    "mahjong",
    "riichi",
    "game"
  ],
  "author": "Takafumi Sakai",
  "homepage": "https://github.com/SakaiTaka23/mjai-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SakaiTaka23/mjai-ts.git"
  },
  "packageManager": "pnpm@10.6.4",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./types": {
      "import": "./packages/types/dist/index.mjs",
      "types": "./packages/types/dist/index.d.ts"
    },
    "./parser": {
      "import": "./packages/parser/dist/index.js",
      "types": "./packages/parser/dist/index.d.ts",
      "require": "./packages/parser/dist/index.cjs"
    },
    "./core": {
      "import": "./packages/core/dist/index.mjs",
      "types": "./packages/core/dist/index.d.ts",
      "require": "./packages/core/dist/index.cjs"
    }
  },
  "scripts": {
    "types": "pnpm -F \"types\"",
    "core": "pnpm -F \"core\"",
    "parser": "pnpm -F \"parser\"",
    "test": "vitest",
    "build": "pnpm run build:types && pnpm run build:packages && pnpm run build:root",
    "build:types": "pnpm -F \"types\" build",
    "build:packages": "pnpm -F \"core\" build && pnpm -F \"parser\" build",
    "build:root": "tsup --dts-resolve"
  },
  "devDependencies": {
    "@mjai/eslint-config": "workspace:^",
    "@mjai/prettier-config": "workspace:^",
    "@mjai/tsconfig-config": "workspace:^",
    "eslint": "^9.26.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "zod": "^4.1.0"
  }
}
