{
  "name": "mjai-ts",
  "version": "0.1.9",
  "description": "TypeScript library for parsing and managing mjai format mahjong game logs with core game state management",
  "license": "MIT",
  "keywords": [
    "mahjong",
    "riichi",
    "game"
  ],
  "author": "Takafumi Sakai",
  "homepage": "https://github.com/SakaiTaka23/mjai-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SakaiTaka23/mjai-ts.git"
  },
  "type": "module",
  "packageManager": "pnpm@10.6.4",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest"
  },
  "dependencies": {
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  }
}
