{
  "extends": "@mjai/tsconfig-config/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@": [
        "../src"
      ],
      "@/*": [
        "../src/*"
      ],
      "@mjai/types": [
        "../types/dist"
      ]
    },
    "outDir": "dist",
    "rootDir": "src"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "../.dist"
  ]
}
